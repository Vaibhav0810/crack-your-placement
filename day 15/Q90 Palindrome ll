Brute Force

  bool isPalindrome(ListNode* head) {
        
        vector<int>v;
        ListNode* temp=head;
        while(temp){
            v.push_back(temp->val);
            temp=temp->next;
        }
        int n=v.size();
        
        for(int i=0;i<n/2;i++){
            if(v[i]!=v[n-i-1]){
                return false;
            }
            
        }
        return true;
        
    }
    
    

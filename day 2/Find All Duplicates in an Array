// brute

class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        
        int i=0;
        
        vector<int>ans;
        for(int j=i+1;j<nums.size();j++){
            
            if(nums[i]==nums[j]){
                ans.push_back(nums[i]);
                
            }
            if(j==nums.size()-1){
                i++;
                j=i;
               
            }
            
        }
        return ans;
    }
};

//optimised
